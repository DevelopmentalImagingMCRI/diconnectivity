def extractLabels(inIMG = None, Labels = None, binarise = False):

	#print Labels
	#if isinstance(Labels, numpy.array):
	#	for i in range(Label.size):
	#		Remap[Labels[i]] = i + 1
#	Remap = dict()
#	LabelsAll = numpy.array([])
